  "devDependencies": {
    "@seabass/eslint-config": "1.8.6",
    "babel-eslint": "7.0.0",
    "eslint": "3.8.0",
    "firebase-tools": "^3.6.1",
    "http-server": "^0.9.0",
    "rollup": "0.36.3",
